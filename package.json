{
  "name": "kirby-content-translator",
  "type": "module",
  "version": "3.8.4",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "dev": "PLAYGROUND=true kirbyup serve src/panel/index.ts",
    "build": "kirbyup src/panel/index.ts",
    "build:playground": "PLAYGROUND=true pnpm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "pnpm run build && bumpp --all --execute \"npx zx scripts/pre-release.mjs\" && zx scripts/post-release.mjs"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^3.0.12",
    "@ai-sdk/google": "^3.0.7",
    "@ai-sdk/mistral": "^3.0.6",
    "@ai-sdk/openai": "^3.0.9",
    "@ai-sdk/provider": "^3.0.2",
    "@antfu/eslint-config": "^6.7.3",
    "@kirby-tools/licensing": "^0.5.2",
    "@sindresorhus/slugify": "^3.0.0",
    "ai": "^6.0.33",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "kirby-types": "^1.4.5",
    "kirbyup": "^3.4.1",
    "kirbyuse": "^1.2.7",
    "ofetch": "^1.5.1",
    "p-all": "^5.0.1",
    "p-map": "^7.0.4",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "ufo": "^1.6.2",
    "vite": "~6.3.7",
    "vitest": "^4.0.17",
    "vue": "^2.7.16",
    "yaml": "^2.8.2",
    "zod": "^4.3.5",
    "zx": "^8.8.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  }
}
